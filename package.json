{
  "name": "axite",
  "description": "Axite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean --parallel && rimraf -G node_modules/{*,.bin,.modules.yaml}",
    "dev": "turbo run dev --parallel",
    "dev:www": "turbo run dev --filter=www --parallel",
    "lint": "turbo run lint",
    "typecheck": "turbo --continue typecheck",
    "format": "prettier --cache --write '{apps,lib}/**/*.{js,jsx,ts,tsx,css,md,mdx,json}'"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "eslint": "^9.0.0",
    "prettier": "3.2.4",
    "rimraf": "^6.0.0",
    "sass": "^1.72.0",
    "supports-color": "^8.0.0",
    "tailwindcss": "3.4.1",
    "turbo": "2.3.3",
    "typescript": "~5.9.0"
  },
  "engines": {
    "pnpm": "10.24",
    "node": ">=22"
  },
  "packageManager": "pnpm@10.24.0"
}
