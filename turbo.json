{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "www#build": {
      "dependsOn": ["^build"],
      "env": [
        "ANALYZE",
        "CI",
        "NODE_ENV",
        "VERCEL",
        "VERCEL_ENV",
        "VERCEL_GIT_COMMIT_SHA",
        "NEXT_PUBLIC_VERCEL_ENV",
        "NEXT_PUBLIC_VERCEL_BRANCH_URL",
        "NEXT_PUBLIC_SENTRY_DSN",
        "NEXT_PUBLIC_HCAPTCHA_SITE_KEY",
        "HCAPTCHA_SECRET_KEY",
        "OPENAI_API_KEY"
      ],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "outputs": []
    },
    "clean": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "test": {
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": []
    }
  }
}
